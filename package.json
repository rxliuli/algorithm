{
  "name": "algorithm",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "yarn commitlint --edit $1"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,css,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/util/extend.ts"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@liuli-util/array": "^3.1.0",
    "@liuli-util/async": "^3.1.1",
    "@liuli-util/test": "^3.1.1",
    "@liuli-util/tree": "^3.1.0",
    "@types/benchmark": "^2.1.0",
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.10",
    "@types/mockjs": "^1.0.3",
    "@types/node": "^14.11.2",
    "benchmark": "^2.1.4",
    "chalk": "^4.1.0",
    "cloc": "^2.6.0",
    "fs-extra": "^9.1.0",
    "husky": "^4.3.8",
    "jest": "^26.4.1",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.1.1",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.1",
    "type-fest": "^0.21.2",
    "typescript": "^4.1.5"
  }
}
